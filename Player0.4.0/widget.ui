QSlider::sub-page:horizontal {
    /* 使用线性渐变实现已通过部分的渐变效果 */
    background: qlineargradient(
        spread: pad, /* 填充模式 */
        x1: 0, y1: 0, /* 起点位置 */
        x2: 1, y2: 0, /* 终点位置（水平渐变） */
        stop: 0 #FC3D49, /* 起始颜色 */
